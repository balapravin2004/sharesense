{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "strict": true,
    "noEmit": false,
    "isolatedModules": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "include": ["server.mts"]
}
